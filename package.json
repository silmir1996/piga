{
  "name": "mcp-playwright-boca",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "codegen:local": "npx playwright codegen http://localhost:3000/",
    "codegen:staging": "npx playwright codegen https://bocasocios-tst.bocajuniors.com.ar/",
    "codegen:dev": "npx playwright codegen https://bocasocios-test.bocajuniors.com.ar/",
    "test:reserva": "npx playwright test tests/e2e/matches/vitalicios/reservaWeb/ --headed",
    "test:reserva-web": "npx playwright test \"tests/e2e/matches/vitalicios/reservaWeb/reservaConValidacion.spec.ts\" --headed",
    "test:desktop": "npx playwright test --project=\"Desktop Chrome\"",
    "test:mobile": "npx playwright test --project=\"Mobile Chrome\"",
    "test:mobile-safari": "npx playwright test --project=\"Mobile Safari\"",
    "test:all-devices": "npx playwright test",
    "test:desktop-mobile": "npx playwright test \"tests/e2e/matches/vitalicios/reservaInterna/4.reservaGeneralesConValidacion.spec.ts\" --project=\"Desktop Chrome\" && npx playwright test \"tests/e2e/matches/vitalicios/reservaInterna/4.reservaGeneralesConValidacion.spec.ts\" --project=\"Mobile Safari\"",
    "mobile": "npx playwright test --project=\"Mobile Safari\"",
    "mobileh": "npx playwright test --project=\"Mobile Safari\" --headed",
    "mobiles": "npx playwright test --project=\"Mobile Safari\" --headed"
  },
